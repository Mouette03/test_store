{
  "services": [
    {
      "name": "phpmyadmin",
      "image": "phpmyadmin:5.2-apache",
      "internalPort": 80,
      "isMain": true,
      "environment": {
        "PMA_ARBITRARY": "1",
        "PMA_PORTS": "${PMA_PORTS}"
      },
       "networksJoin": [
        "runtipi_tipi_main_network",
        "dolibarr_damien_network",
        "galette_damien_default"
      ]
    }
  ]
}
