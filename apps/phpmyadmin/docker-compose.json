{
  "services": [
    {
      "name": "phpmyadmin",
      "image": "phpmyadmin:5.2-apache",
      "internalPort": 80,
      "isMain": true,
      "environment": {
        "PMA_HOSTS": "${PMA_HOSTS}",
        "PMA_PORTS": "${PMA_PORTS}"
      },
      "networks": [
        "runtipi_tipi_main_network",
        "webapp_damien_network"
      ]
    }
  ],
  "networks": {
    "runtipi_tipi_main_network": {
      "external": true
    },
    "webapp_damien_network": {
      "external": true
    }
  }
}
