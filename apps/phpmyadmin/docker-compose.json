{
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/dynamic-compose.json",
  "services": [
    {
      "name": "adminer",
      "image": "adminer:4.8.1-standalone",
      "internalPort": 8080,
      "isMain": true,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/var/www/html/data",
          "readOnly": false
        }
      ]
    }
  ]
}
