{
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/dynamic-compose.json",
  "services": [
    {
      "name": "netronome",
      "image": "ghcr.io/autobrr/netronome:v0.8.0",
      "internalPort": 7575,
      "isMain": true,
      "command": "sh -c 'mkdir -p /data && netronome serve'",
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/data"
        }
      ]
    }
  ]
}
