{
  "services": [
    {
      "name": "palmr",
      "image": "kyantech/palmr:v3.2.2-beta",
      "internalPort": 5487,
      "isMain": true,
      "environment": {
        "DOWNLOAD_MAX_CONCURRENT": "${DOWNLOAD_MAX_CONCURRENT}",
        "SECURE_SITE": "${SECURE_SITE}",
        "DOWNLOAD_MIN_FILE_SIZE_GB": "${ACTIVATE_MEMORY_MANAGEMENT}",
        "DEFAULT_LANGUAGE": "${DEFAULT_LANGUAGE}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/server",
          "containerPath": "/app/server"
        }
      ]
    }
  ]
}